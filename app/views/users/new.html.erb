<h1>Make an account and start reviewing wines today!</h1>
<h3>Signing up is EASY</h3>

<%= render partial: 'layouts/errors', locals: { object: @user } %>

<div>
<%= form_for @user do |f|%>
<p>
<%= f.label :username %>
<%= f.text_field :username %></p>

<p>
<%= f.label :password %>
<%= f.password_field :password %>
</p>

<%= f.submit %>
<% end %>

</div>